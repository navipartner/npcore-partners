{
  "build": {
    "content": [
      {
        "files": [
          "api/**.md",
          "api/**/toc.yml",                    
          "product/**.md",
          "product/**/toc.yml",                    
          "toc.yml",
          "*.md"          
        ],
        "src": "../../public"
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "**/images/**"
        ],
        "src": "../../public"
      }
    ],
    "markdownEngineName": "markdig",
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/darkfx"    
    ],
    "postProcessors": [
      "ExtractSearchIndex"
    ],
    "globalMetadata": {
      "_appFooter": "<span>NaviPartner Retail  -  Site generated using DocFx</span>",
      "_appTitle": "NP Retail",
      "_enableSearch": "true",
      "_appFaviconPath": "images/favicon.ico"
    },
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": true
  }
}